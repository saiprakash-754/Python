
num1=int(input("num1: "))
num2=int(input("num2: "))
add=int(num1+num2)
sub=int(num1-num2)
multiply=int(num1*num2)
division=float(num1/num2)
print('Addition of',num1,"and",num2,'=',add)
print("Subtraction of",num1,"and",num2,"=",sub)
print("Multiplication of",num1,"and",num2,"=",multiply)
print("Division of",num1,"and",num2,"=",division)
